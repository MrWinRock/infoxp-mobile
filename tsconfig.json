{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "6.0",
    "paths": {
      "@/*": ["./*"],
      "@config/*": ["./config/*"],
      "@services/*": ["./services/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
